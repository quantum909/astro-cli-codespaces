# Airflow 3.0 environment variables for GitHub Codespaces
AIRFLOW__WEBSERVER__CSRF_ENABLED=False
AIRFLOW__WEBSERVER__VALIDATE_REDIRECT_URL=False
AIRFLOW__WEBSERVER__ENABLE_PROXY_FIX=True
AIRFLOW__WEBSERVER__ALLOWED_HOSTS=*.app.github.dev,localhost,127.0.0.1
# Disable authentication for development
AIRFLOW__WEBSERVER__AUTHENTICATE=False
AIRFLOW__API__AUTH_BACKENDS=airflow.api.auth.backend.default
