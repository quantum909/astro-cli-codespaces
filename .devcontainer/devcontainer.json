{
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu-24.04",
    "features": {
        "ghcr.io/devcontainers/features/python:1": {
            "version": "3.12"
        },
        "ghcr.io/devcontainers/features/docker-in-docker:2": {
            "moby": "true"
        }
    },
    "postCreateCommand": "curl -sSL install.astronomer.io | sudo bash -s",
    "forwardPorts": [8080],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python"
            ]
        }
    }
}
